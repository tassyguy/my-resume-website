{
  "name": "my-resume-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "predeploy": "next build",
    "deploy": "next export",
    "postdeploy": "node -e \"require('os').platform() === 'win32' ? require('child_process').execSync('powershell -Command \"New-Item -Path out/.nojekyll -ItemType file; Move-Item -Path out/* -Destination .; Remove-Item -Path out -Recurse\"') : require('child_process').execSync('touch out/.nojekyll && mv out/* . && rm -rf out');\""
  },
  "dependencies": {
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "css-loader": "^6.8.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "gh-pages": "^6.0.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}